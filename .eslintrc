{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "testcafe"],
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "prefer-arrow-callback": 0,
    "func-names": 2,
    "react/jsx-filename-extension": 0,
    "react/require-default-props": 0,
    "react/prefer-stateless-function": 0,
    "arrow-body-style": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-named-as-default": 0,
    "object-curly-newline": 0,
    "no-case-declarations": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "ignore"
    }]
  },
  "globals": {
    "__DEV__": true,
    "window": true,
    "document": true,
    "describe": true,
    "it": true,
    "expect": true,
    "beforeEach": true,
    "afterEach": true,
    "before": true,
    "after": true,
    "test": true,
    "jest": true
  }
}
